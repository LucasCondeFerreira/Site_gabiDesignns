{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@styles/*": ["ui/styles/*"],
      "@components/*": ["ui/components/*"],
      "@themes/*": ["ui/themes/*"],
      "@data/*": ["data/*"],
      "@pages/*": ["pages/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/ui/components/surface/header/Header.jsx",
    "src/ui/components/data-display/especificElements/haburguerMenu/HamburguerMenu.jsx",
    "src/ui/components/data-display/forms/form.jsx",
    "src/pages/teste.jsx"
  ],
  "exclude": ["node_modules"]
}
